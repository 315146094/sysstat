#: sar.c:152
#, c-format
msgid "\t-F [ MOUNT ]\n"
msgstr "\t-F [ MOUNT ]\n"

#: sar.c:153
#, c-format
msgid "\t\tFilesystems statistics [A_FS]\n"
msgstr "\t\tStatistiche dei filesystem [A_FS]\n"

